{
  "version": 2,
  "name": "journal-buddy",
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/favicon.ico$",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/manifest.json$",
      "dest": "/manifest.json"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "REACT_APP_API_BASE_URL": "@journal-buddy-api-url"
    }
  }
}